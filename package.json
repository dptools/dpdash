{
  "name": "po",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "transpile": "npx babel ./server --out-dir dist",
    "dev": "NODE_ENV=development concurrently \"npm run serve:watch\" \"npm run build:watch\"",
    "start": "NODE_ENV=production ./node_modules/pm2/bin/pm2 start dist/bin/www.js",
    "server": "nodemon --exec babel-node server/bin/www.js",
    "monitor": "./node_modules/pm2/bin/pm2 monit",
    "stop": "./node_modules/pm2/bin/pm2 stop all && ./node_modules/pm2/bin/pm2 delete all",
    "logs": "./node_modules/pm2/bin/pm2 logs --lines 100",
    "restart": "NODE_ENV=production ./node_modules/pm2/bin/pm2 stop all && ./node_modules/pm2/bin/pm2 delete all && ./node_modules/pm2/bin/pm2 start dist/bin/www.js",
    "serve:watch": "nodemon --exec babel-node server/bin/www.js --watch \"./server\"",
    "node": "DEBUG=* node ./dist/bin/www.js",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-transform-react-constant-elements",
      "@babel/plugin-transform-react-inline-elements"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.3.0",
    "@material-ui/icons": "^3.0.1",
    "amqplib": "^0.8.0",
    "body-parser": "^1.18.3",
