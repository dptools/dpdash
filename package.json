    "co": "^4.6.0",
    "colorbrewer": "^1.3.0",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.3",
    "cron": "^1.4.1",
    "csurf": "^1.11.0",
    "csv-stringify": "^6.3.0",
    "d3": "^5.7.0",
    "debug": "^4.3.2",
    "deep-equal": "^2.2.0",
    "distinct-colors": "^3.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "file-saver": "^2.0.0-rc.4",
    "fs": "0.0.1-security",
    "helmet": "^3.14.0",
    "http-errors": "~1.6.2",
    "immutability-helper": "^2.8.1",
    "lodash": "^4.17.21",
    "material-ui-chip-input": "^1.0.0-beta.8",
    "mathjs": "^7.6.0",
    "method-override": "^3.0.0",
